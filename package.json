{
  "name": "server",
  "version": "1.0.0",
  "description": "App for weekly assignment allocation",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "concurrently \"open http://localhost:4000\" \"node server.js\"",
    "serve": "nodemon server.js",
    "package": "pkg package.json -t node16-win-x64 --out-path dist && cp ./node_modules/open/xdg-open ./dist/xdg-open"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "client-dev-app/**/*"
    ],
    "targets": [
      "node16-macos-x64"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.1",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "kill-port": "^1.6.1",
    "open": "^8.4.0"
  }
}
